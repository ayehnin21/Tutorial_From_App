<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UIWidgets">

    <Button
        android:id="@+id/btnACTextView"
        style="@style/n_btn_style"
        android:layout_marginTop="276dp"
        android:text="Go to Auto complete Text View"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnGotoMultiCTextView"
        style="@style/n_btn_style"
        android:layout_marginTop="56dp"
        android:text="Go to multi complete text view"
        app:layout_constraintEnd_toEndOf="@+id/btnACTextView"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnACTextView"
        app:layout_constraintTop_toBottomOf="@+id/btnACTextView" />
</androidx.constraintlayout.widget.ConstraintLayout>